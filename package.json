{
    "name": "new-cmd",
    "description": "Launch a new command line that target the current workspace",
    "version": "0.2.0",
    "bugs": {
        "url": "https://github.com/Odonno/new-cmd/issues"
    },
    "homepage": "https://github.com/Odonno/new-cmd/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/Odonno/new-cmd.git"
    },
    "icon": "images/logo.png",
    "galleryBanner": {
        "color": "#EAEAEA",
        "theme": "light"
    },
    "publisher": "odonno",
    "engines": {
        "vscode": "^0.10.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.cmd"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.cmd",
                "title": "cmd"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
    },
    "devDependencies": {
        "typescript": "^1.6.2",
        "vscode": "0.10.x"
    }
}