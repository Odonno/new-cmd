{
	"name": "new-cmd",
	"description": "Launch a new command line that target the current workspace",
	"version": "0.1.0",
	"publisher": "odonno",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.launchCmd"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [{
			"command": "extension.launchCmd",
			"title": "Launch cmd"
		}]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}